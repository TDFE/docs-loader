{
  "name": "docs-loader",
  "version": "0.0.1",
  "description": "webpack2插件，用于将md文件转换为react组件",
  "main": "lib/index.js",
  "repository": "https://github.com/TDFE/docs-loader.git",
  "author": "hihl <348267823@qq.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && echo Using Babel && babel --version && babel src --out-dir lib -s"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy",
      "babel-plugin-transform-class-properties",
      "babel-plugin-transform-runtime",
      "babel-plugin-lodash"
    ]
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-generator": "^6.24.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-traverse": "^6.24.1",
    "babel-types": "^6.24.1",
    "jsonml-to-react-element": "^1.0.0",
    "jsonml.js": "^0.1.0",
    "loader-utils": "^1.1.0",
    "mark-twain": "^2.0.1",
    "node-prismjs": "^0.1.0",
    "ramda": "^0.24.1",
    "react": "^15.5.4",
    "react-sublime-video": "^0.2.5",
    "resolve": "^1.3.3",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  }
}
