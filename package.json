{
  "name": "@td-ui/react-markdown-loader",
  "version": "1.0.0",
  "description": "td-ui使用的webpack2插件，用于将md文件转换为react组件",
  "main": "index.js",
  "repository": "https://github.com/hihl/react-markdown-loader.git",
  "author": "hihl <348267823@qq.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && echo Using Babel && babel --version && babel src --out-dir lib -s"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy",
      "babel-plugin-transform-class-properties",
      "babel-plugin-transform-runtime",
      "babel-plugin-lodash"
    ]
  },
  "dependencies": {
    "markdown-it": "^8.3.1",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-container": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  }
}
